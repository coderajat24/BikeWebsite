@model IEnumerable<BikeWebsite.vProductAndDescription>

@{
    ViewBag.Title = "Louis";
}

<h2>Search</h2>

<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Details/" + item.ProductCategoryID.ToString())
            </td>
        </tr>
    }
</table>