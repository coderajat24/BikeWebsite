@model IEnumerable<BikeWebsite.ProductCategory>
@{
    ViewBag.Title = "Accessories";
}

<h2>Accessories</h2>

<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Details/" + item.ProductCategoryID.ToString())
            </td>
        </tr>
    }
</table>